{
  "watch": [
    "server.js",
    "auth/",
    "public/",
    "scripts/"
  ],
  "ext": "js,html,css,json",
  "ignore": [
    "node_modules/",
    "uploads/",
    "*.log",
    "public/images/"
  ],
  "delay": "1000",
  "verbose": true,
  "env": {
    "NODE_ENV": "development",
    "PORT": 3008
  },
  "exec": "node"
}